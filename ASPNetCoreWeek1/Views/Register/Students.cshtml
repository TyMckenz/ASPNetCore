@model List<Student>
@{
    ViewData["Title"] = "Students";
}

@*Made a second table to be able to access the students in a for each loop, to save time there. 
However it made me have to write another list of students because I couldn't figure out how to
access them from inside the course constructor*@

<div class="text-center">
    <h1 class ="display-4">List Of Students</h1>
    <table class="table">
        <thead>
            <tr>
                <th>Student ID</th>
                <th>Student First Name</th>
                <th>Student Last Name</th>
                <th>Student Email</th>
                <th>Student Phone Number</th>
            </tr>
        </thead>
        <tbody>
                        
            @foreach (var student in Model)
            {

               <tr>
                  <td>@student.StuFirstName</td>
                  <td>@student.StuLastName</td>
                  <td>@student.StudentId</td>
                  <td>@student.Email</td>
                  <td>@student.PhoneNumber</td>
            </tr>

            }
        </tbody>
    </table>

 </div>

